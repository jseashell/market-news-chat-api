{
  "name": "finnhub-api",
  "version": "0.0.1",
  "description": "Serverless Websocket API providing Finnhub live data",
  "author": "John Schellinger",
  "private": true,
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "npx prettier --write *.js --ignore-path package*.json"
  },
  "dependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.87.0",
    "@aws-sdk/client-dynamodb": "^3.87.0"
  },
  "devDependencies": {
    "jest": "^28.1.0",
    "prettier": "^2.6.2",
    "serverless": "^3.17.0"
  },
  "jest": {
    "rootDir": ".",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "text-summary"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "./setupTests.js"
    ]
  }
}
